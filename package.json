{
  "name": "template-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/module/index.js",
  "types": "dist/module/index.d.ts",
  "scripts": {
    "build": "run-s clean build-all",
    "build-all": "run-p build:*",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build-module": "run-p build:tsc build:module",
    "build:tsc": "tsc",
    "build:module": "babel src --extensions \".ts\" --out-dir dist/module --source-maps inline",
    "clean": "run-p clean:*",
    "clean:module": "rimraf dist/module",
    "clean:browser": "rimraf dist/browser",
    "watch": "run-p watch:*",
    "watch:webpack": "webpack --watch",
    "watch:tsc": "tsc --watch",
    "test": "run-s test:run:*",
    "test:run:jest": "jest",
    "test:coverage": "jest --collect-coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "prepublishOnly": "run-s clean:module build-module"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^3.9.9",
    "webpack": "^5.27.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {},
  "eslintIgnore": [
    "dist/**/*"
  ],
  "jest": {
    "roots": [
      "./test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(test).[jt]s"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./test/tsconfig.json"
      }
    },
    "testEnvironment": "./test/Environment.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/test/"
    ]
  }
}
